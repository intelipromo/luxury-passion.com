{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=5.5.9",
        "laravel/framework": "5.2.*",
        "plentymarkets/plugin-interface": "dev-beta7",
        "plentymarkets/plugin-io": "1.0",
        "plentymarkets/plugin-ceres": "1.0",
        "plentymarkets/terra-components": "1.0"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~4.0",
        "symfony/css-selector": "2.8.*|3.0.*",
        "symfony/dom-crawler": "2.8.*|3.0.*"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php"
        ]
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "post-install-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postInstall",
            "php artisan optimize"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "php artisan optimize"
        ]
    },
    "config": {
        "preferred-install": "dist"
    },
    "extra": {
        "plentymarkets": {
            "providers": [
                "Ceres\\Providers\\TemplateServiceProvider"
            ]
        }
    },
    "repositories": [
    {
        "type": "package",
        "package": {
            "name": "plentymarkets/plugin-io",
            "version": "1.0",
            "source": {
                "url": "https://github.com/plentymarkets/plugin-io.git",
                "type": "git",
                "reference": "stable"
            }
        }
    },
    {
        "type": "package",
        "package": {
            "name": "plentymarkets/plugin-ceres",
            "version": "1.0",
            "source": {
                "url": "https://github.com/plentymarkets/plugin-ceres.git",
                "type": "git",
                "reference": "stable"
            }
        }
    },
    {
        "type": "package",
        "package": {
            "name": "plentymarkets/terra-components",
            "version": "1.0",
            "source": {
                "url": "https://github.com/plentymarkets/terra-components.git",
                "type": "git",
                "reference": "master"
            }
        }
    }
    ]
}
